// Questo sommatore utilizza solo 4 bit, eventuale overflow viene ignorato

.model SUM4
.inputs A3 A2 A1 A0 B3 B2 B1 B0
.outputs OUT3 OUT2 OUT1 OUT0

.subckt A=A0 B=B0 CIN=0 OUT=OUT0 COUT=COUT0
.subckt A=A1 B=B1 CIN=COUT0 OUT=OUT1 COUT=COUT1
.subckt A=A2 B=B2 CIN=COUT1 OUT=OUT2 COUT=COUT2
.subckt A=A3 B=B3 CIN=COUT2 OUT=OUT3 COUT=0 // TODO: Check how ingnore the output

.search sum.blif
.end
